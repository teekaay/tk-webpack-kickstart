{
  "name": "webpack-redux-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/bundle.js",
  "scripts": {
    "bundle:dist": "npm run bundle:clean && npm run bundle:js",
    "bundle:js": "NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "bundle:watch": "node server.js",
    "bundle:profile": "./node_modules/.bin/webpack -p --profile --json > stats.json",
    "bundle:clean": "rm -rf dist",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "test": "./node_modules/.bin/jest --config .jestrc",
    "test:watch": "npm run test --watch"
  },
  "author": "Thomas Klinger",
  "license": "ISC",
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "jest": "^18.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  jest: {
    "bail": true,
    "verbose": true,
    "notify": true
  },
  babel: {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
